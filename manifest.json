{
  "manifest_version": 2,

  "name": "Moan Keys",
  "description": "WIll sound like the whatsapp moaning woman when pressing the keys: M O A N",
  "version": "1.0",

  "content_scripts": [
      {
          "js": ["lib/jquery-2.1.1.min.js", "moan.js"],
          "matches": ["http://*/*", "https://*/*"]
      }
  ],

  "browser_action": {
   "default_icon": "moan.png",
   "default_popup": "popup.html"
  },
  "permissions": [
      "storage",
      "tabs",
      "contextMenus",
      "webRequest",
      "<all_urls>",
      "activeTab",
      "http://*/*",
      "https://*/*",
      "webNavigation",
      "webRequestBlocking",
      "cookies",
      "storage",
      "unlimitedStorage"
  ]
}
